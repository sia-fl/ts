{
	"semi": true,
	"printWidth": 100,
	"singleQuote": true,
	"bracketSpacing": true,
	"insertPragma": false,
	"requirePragma": false,
	"jsxSingleQuote": false,
	"bracketSameLine": false,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"vueIndentScriptAndStyle": true,
	"quoteProps": "consistent",
	"proseWrap": "preserve",
	"trailingComma": "none",
	"arrowParens": "avoid",
	"useTabs": true,
	"tabWidth": 2,
	"endOfLine": "auto"
}
